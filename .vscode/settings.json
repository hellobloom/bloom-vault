{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules/*/**": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.trimAutoWhitespace": true,
  "files.trimTrailingWhitespace": true
}
